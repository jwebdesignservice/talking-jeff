{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    },
    {
      "src": "Images/**",
      "use": "@vercel/static"
    },
    {
      "src": "video animation/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/css/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/js/$1"
    },
    {
      "src": "/Images/(.*)",
      "dest": "/Images/$1"
    },
    {
      "src": "/video%20animation/(.*)",
      "dest": "/video animation/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
